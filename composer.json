{
    "name": "collecthor/flysystem-adapters",
    "description": "Meta adapters for FlySystem v3",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "league/flysystem": "^3.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "league/flysystem-adapter-test-utilities": "^3.0",
        "phpunit/phpunit": "^10",
        "league/flysystem-memory": "^3.0",
        "symplify/easy-coding-standard": "^12",
        "captainhook/plugin-composer": "^5.3",
        "ramsey/conventional-commits": "^1.3",
        "vimeo/psalm": "dev-master",
        "phpstan/phpstan": "^1.10",
        "ekino/phpstan-banned-code": "^2.1",
        "phpstan/extension-installer": "^1.3",
        "league/event": "^3.0"
    },
    "suggest": {
        "psr/event-dispatcher-implementations": "*"
    },
    "autoload": {
        "psr-4": {
            "Collecthor\\FlySystem\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Collecthor\\FlySystem\\Tests\\": ["tests/"]
        }
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "composer/package-versions-deprecated": false,
            "phpstan/extension-installer": true
        }
    }
}
